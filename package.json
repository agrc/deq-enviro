{
  "name": "deq-enviro",
  "version": "2.0.0-8",
  "private": true,
  "description": "DEQ Interactive Map",
  "license": "MIT",
  "author": "UGRC",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "build:dev": "npm run build:dev:update-remote-config-defaults && npm run build && npm run build:storybook",
    "build:dev:update-remote-config-defaults": "export GOOGLE_APPLICATION_CREDENTIALS=\"$PWD/functions/functions-key-dev.json\" && node build-scripts/updateRemoteConfigDefaults.js",
    "build:prod": "npm run build",
    "build:stage": "npm run build && npm run build:storybook",
    "build:storybook": "storybook build --output-dir ./dist/storybook",
    "build:update-remote-config-defaults": "node build-scripts/updateRemoteConfigDefaults.js",
    "copy:arcgis": "cpy \"./node_modules/@arcgis/core/assets/**\" ./public/assets",
    "dev": "cross-env NODE_ENV=development run-p dev:*",
    "dev:firebase": "export GOOGLE_APPLICATION_CREDENTIALS=\"$PWD/functions/functions-key-dev.json\" && firebase emulators:start --import .emulator-data --only functions,storage",
    "dev:vite": "vite",
    "format": "prettier --write .",
    "lint": "eslint ./ --ext .js,.jsx --ignore-path ./.eslintignore",
    "lint:fix": "eslint ./ --ext .js,.jsx --quiet --fix --ignore-path ./.eslintignore",
    "preview": "vite preview",
    "save-firebase-state": "firebase emulators:start --import .emulator-data --export-on-exit --only functions",
    "start": "npm run dev",
    "test": "vitest --coverage",
    "coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6006"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "semi": true,
    "singleQuote": true
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true,
      "jest": true,
      "node": true
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "plugins": [
      "react",
      "prettier",
      "jsx-a11y",
      "react-hooks",
      "react-refresh"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react/jsx-runtime",
      "plugin:react-hooks/recommended",
      "plugin:jsx-a11y/recommended",
      "prettier"
    ],
    "rules": {
      "react-refresh/only-export-components": "warn"
    }
  },
  "dependencies": {
    "@arcgis/core": "^4.26.5",
    "@radix-ui/react-accessible-icon": "^1.0.3",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-tooltip": "^1.0.6",
    "@tanstack/react-query": "^4.29.12",
    "@tanstack/react-table": "^8.9.1",
    "@ugrc/layer-selector": "^6.1.1",
    "@ugrc/utilities": "^2.2.1",
    "@utahdts/utah-design-system-header": "^0.8.0",
    "@xstate/react": "^3.2.2",
    "clsx": "^1.2.1",
    "firebase-admin": "^11.9.0",
    "googleapis": "^118.0.0",
    "ky": "^0.33.3",
    "localforage": "^1.10.0",
    "react": "^18.2.0",
    "react-content-loader": "^6.2.1",
    "react-dom": "^18.2.0",
    "react-virtual": "^2.10.4",
    "reactfire": "^4.2.2",
    "tailwind-merge": "^1.13.0",
    "xstate": "^4.37.2",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.18",
    "@storybook/addon-interactions": "^7.0.18",
    "@storybook/addon-links": "^7.0.18",
    "@storybook/addon-styling": "^1.0.8",
    "@storybook/blocks": "^7.0.18",
    "@storybook/react": "^7.0.18",
    "@storybook/react-vite": "^7.0.18",
    "@storybook/testing-library": "^0.1.0",
    "@tailwindcss/forms": "^0.5.3",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-c8": "^0.31.4",
    "autoprefixer": "^10.4.14",
    "cpy-cli": "^4.2.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.1.3",
    "eslint": "^8.41.0",
    "eslint-config-plugin": "^1.0.11",
    "eslint-plugin-pretty": "^0.0.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "firebase-tools": "^12.3.0",
    "if-env": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^3.2.2",
    "prettier-plugin-packagejson": "^2.4.3",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "prop-types": "^15.8.1",
    "sass": "^1.62.1",
    "storybook": "^7.0.18",
    "tailwindcss": "^3.3.2",
    "vite": "^4.3.9",
    "vitest": "^0.31.4"
  },
  "overrides": {
    "react-virtual": {
      "react": "$react"
    }
  }
}
