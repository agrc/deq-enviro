{
  "name": "deq-enviro",
  "version": "2.0.29",
  "private": true,
  "description": "DEQ Interactive Map",
  "license": "MIT",
  "author": "UGRC",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "build:cloudrun": "docker build ./cloudrun --target dev -t download",
    "build:dev": "pnpm run build:update-remote-config-defaults && pnpm run build && pnpm run build:storybook",
    "build:prod": "pnpm run build:update-remote-config-defaults && pnpm run build",
    "build:stage": "pnpm run build:update-remote-config-defaults && pnpm run build && pnpm run build:storybook",
    "build:storybook": "storybook build --output-dir ./dist/storybook",
    "build:update-remote-config-defaults": "node build-scripts/updateRemoteConfigDefaults.js",
    "copy:arcgis": "cpy \"./node_modules/@arcgis/core/assets/**\" ./public/assets",
    "dev": "cross-env NODE_ENV=development concurrently 'pnpm run dev:firebase' 'pnpm run dev:vite' 'pnpm run dev:cloudrun'",
    "dev:cloudrun": "docker run -p 5010:5010 --mount type=bind,source=$HOME/.config/gcloud,target=/gcp/config,readonly download",
    "dev:firebase": "export GOOGLE_APPLICATION_CREDENTIALS=\"$PWD/functions/functions-key-dev.json\" && firebase emulators:start --import .emulator-data --only functions,storage,firestore",
    "dev:vite": "vite",
    "format": "prettier --write .",
    "postinstall": "pnpm run copy:arcgis",
    "lint": "eslint ./ && tsc",
    "lint:fix": "eslint ./ --quiet --fix",
    "preview": "vite preview",
    "save-firebase-state": "firebase emulators:start --import .emulator-data --export-on-exit --only functions",
    "start": "pnpm run dev",
    "storybook": "storybook dev -p 6006",
    "test": "vitest"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-organize-imports",
      "./node_modules/prettier-plugin-jsdoc/dist/index.js",
      "prettier-plugin-packagejson",
      "prettier-plugin-tailwindcss"
    ],
    "semi": true,
    "singleQuote": true
  },
  "overrides": {
    "react-virtual": {
      "react": "$react"
    }
  },
  "dependencies": {
    "@arcgis/core": "^4.33.14",
    "@esri/calcite-components": "^3.3.3",
    "@esri/calcite-components-react": "^3.3.3",
    "@radix-ui/react-accessible-icon": "^1.1.7",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-table": "^8.21.3",
    "@ugrc/esri-theme-toggle": "^1.1.2",
    "@ugrc/utah-design-system": "^1.28.2",
    "@utahdts/utah-design-system-header": "^4.1.0",
    "@xstate/react": "^6.0.0",
    "clsx": "^2.1.1",
    "downshift": "^9.0.10",
    "firebase": "^12.3.0",
    "googleapis": "^164.1.0",
    "immer": "^10.2.0",
    "ky": "^1.10.0",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.553.0",
    "react": "^19.2.0",
    "react-content-loader": "^7.1.1",
    "react-dom": "^19.2.0",
    "react-draggable": "^4.5.0",
    "react-error-boundary": "^6.0.0",
    "react-virtual": "^2.10.4",
    "tailwind-merge": "^3.3.1",
    "use-immer": "^0.11.0",
    "xstate": "^5.22.0",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.2",
    "@dotenvx/dotenvx": "^1.48.0",
    "@storybook/addon-docs": "^10.0.6",
    "@storybook/addon-links": "^10.0.6",
    "@storybook/react-vite": "^10.0.6",
    "@tailwindcss/forms": "^0.5.10",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@ugrc/eslint-config": "^1.2.3",
    "@ugrc/tailwind-preset": "^1.2.0",
    "@ugrc/tsconfigs": "^1.0.3",
    "@vitejs/plugin-react": "^5.1.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "cpy-cli": "^6.0.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.36.0",
    "eslint-plugin-storybook": "10.0.6",
    "firebase-admin": "^13.5.0",
    "firebase-tools": "^14.17.0",
    "happy-dom": "^18.0.1",
    "if-env": "^1.0.4",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-jsdoc": "^1.3.3",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-packagejson": "^2.5.19",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "prop-types": "^15.8.1",
    "sass": "^1.89.2",
    "storybook": "^10.0.6",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.9.3",
    "vite": "^6.3.6",
    "vitest": "^3.2.4"
  }
}
