{
  "name": "deq-enviro",
  "version": "2.0.19",
  "private": true,
  "description": "DEQ Interactive Map",
  "license": "MIT",
  "author": "UGRC",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "build:cloudrun": "docker build ./cloudrun --target dev -t download",
    "build:dev": "npm run build:update-remote-config-defaults && npm run build && npm run build:storybook",
    "build:prod": "npm run build:update-remote-config-defaults && npm run build",
    "build:stage": "npm run build:update-remote-config-defaults && npm run build && npm run build:storybook",
    "build:storybook": "storybook build --output-dir ./dist/storybook",
    "build:update-remote-config-defaults": "node build-scripts/updateRemoteConfigDefaults.js",
    "copy:arcgis": "cpy \"./node_modules/@arcgis/core/assets/**\" ./public/assets",
    "dev": "cross-env NODE_ENV=development concurrently 'npm run dev:firebase' 'npm run dev:vite' 'npm run dev:cloudrun'",
    "dev:cloudrun": "docker run -p 5000:5000 --mount type=bind,source=$HOME/.config/gcloud,target=/gcp/config,readonly download",
    "dev:firebase": "export GOOGLE_APPLICATION_CREDENTIALS=\"$PWD/functions/functions-key-dev.json\" && firebase emulators:start --import .emulator-data --only functions,storage,firestore",
    "dev:vite": "vite",
    "format": "prettier --write .",
    "lint": "eslint ./ && tsc",
    "lint:fix": "eslint ./ --quiet --fix",
    "preview": "vite preview",
    "save-firebase-state": "firebase emulators:start --import .emulator-data --export-on-exit --only functions",
    "start": "npm run dev",
    "storybook": "storybook dev -p 9009",
    "test": "vitest"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-organize-imports",
      "./node_modules/prettier-plugin-jsdoc/dist/index.js",
      "prettier-plugin-packagejson",
      "prettier-plugin-tailwindcss"
    ],
    "semi": true,
    "singleQuote": true
  },
  "dependencies": {
    "@arcgis/core": "^4.31.6",
    "@esri/calcite-components-react": "^2.13.2",
    "@radix-ui/react-accessible-icon": "^1.1.1",
    "@radix-ui/react-accordion": "^1.2.2",
    "@radix-ui/react-checkbox": "^1.1.3",
    "@radix-ui/react-collapsible": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-popover": "^1.1.4",
    "@radix-ui/react-radio-group": "^1.2.2",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-tabs": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.6",
    "@tanstack/react-query": "^5.64.1",
    "@tanstack/react-table": "^8.20.6",
    "@ugrc/layer-selector": "^6.2.12",
    "@utahdts/utah-design-system-header": "^3.0.3",
    "@xstate/react": "^5.0.2",
    "clsx": "^2.1.1",
    "downshift": "^9.0.8",
    "googleapis": "^144.0.0",
    "immer": "^10.1.1",
    "ky": "^1.7.4",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "react": "^19.0.0",
    "react-content-loader": "^7.0.2",
    "react-dom": "^19.0.0",
    "react-draggable": "^4.4.6",
    "react-error-boundary": "^5.0.0",
    "react-virtual": "^2.10.4",
    "reactfire": "^4.2.3",
    "tailwind-merge": "^2.6.0",
    "use-immer": "^0.11.0",
    "xstate": "^5.19.1",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.3",
    "@dotenvx/dotenvx": "^1.35.0",
    "@eslint/compat": "^1.2.5",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@storybook/addon-essentials": "^8.5.0",
    "@storybook/addon-interactions": "^8.5.0",
    "@storybook/addon-links": "^8.5.0",
    "@storybook/blocks": "^8.4.7",
    "@storybook/react": "^8.5.0",
    "@storybook/react-vite": "^8.5.0",
    "@storybook/test": "^8.4.7",
    "@tailwindcss/forms": "^0.5.10",
    "@types/eslint__js": "^8.42.3",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@ugrc/tsconfigs": "^1.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "cpy-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.18.0",
    "eslint-config-plugin": "^1.0.11",
    "eslint-plugin-pretty": "^0.0.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "firebase-admin": "^13.0.2",
    "firebase-tools": "^13.29.1",
    "globals": "^15.14.0",
    "if-env": "^1.0.4",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "prettier-plugin-jsdoc": "^1.3.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.7",
    "prettier-plugin-tailwindcss": "^0.6.10",
    "prop-types": "^15.8.1",
    "sass": "^1.83.4",
    "storybook": "^8.4.7",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.0.7",
    "vitest": "^2.1.8"
  },
  "overrides": {
    "react-virtual": {
      "react": "$react"
    }
  }
}
